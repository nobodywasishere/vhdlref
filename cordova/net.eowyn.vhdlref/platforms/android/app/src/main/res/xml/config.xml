<?xml version='1.0' encoding='utf-8'?>
<widget id="net.eowyn.vhdlref" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>VHDLref</name>
    <description>
        A usable language reference for writing VHDL that is concise, direct, and easy to understand.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="ldpi" src="../../assets/icons/android-icon-36x36.png" />
    <icon density="mdpi" src="../../assets/icons/android-icon-48x48.png" />
    <icon density="hdpi" src="../../assets/icons/android-icon-72x72.png" />
    <icon density="xhdpi" src="../../assets/icons/android-icon-96x96.png" />
    <icon density="xxhdpi" src="../../assets/icons/android-icon-144x144.png" />
    <icon density="xxxhdpi" src="../../assets/icons/android-icon-192x192.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
